{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build XpressFormula (x64 Debug)",
      "type": "shell",
      "command": "powershell",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy",
        "Bypass",
        "-File",
        "${workspaceFolder}\\scripts\\invoke-msbuild.ps1",
        "-ProjectPath",
        "src\\XpressFormula.slnx",
        "-Configuration",
        "Debug",
        "-Platform",
        "x64",
        "-Targets",
        "Build"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$msCompile"
      ]
    },
    {
      "label": "Build XpressFormula (x64 Release)",
